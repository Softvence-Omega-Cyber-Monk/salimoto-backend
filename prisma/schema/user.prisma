model User {
    id                   String     @id @default(uuid())
    firstname            String
    lastname             String
    username             String     @unique
    fcmToken             String?
    password             String
    isActive             Boolean    @default(false)
    isDeleted            Boolean    @default(false)
    createdAt            DateTime   @default(now())
    updatedAt            DateTime   @updatedAt
}